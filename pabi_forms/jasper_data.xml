<?xml version="1.0"?>
<openerp>
<data>
	
	# Customer Invoice 
	
	<!-- Delete existing report -->
	<delete model="ir.actions.report.xml" search="[('id', '=', ref('account.account_invoices'))]" />

	<record id="nstda_msd_invoice" model="ir.actions.report.xml">
		<field name="string">Invoice</field>
		<field name="auto">True</field>
		<field name="model">account.invoice</field>
		<field name="jasper_model_id" ref="account.model_account_invoice"/>
		<field name="name">Invoice</field>
		<field name="report_name">nstda.msd.invoice</field>
		<field name="report_rml">pabi_forms/jrxml_reports/nstda_msd_invoice.jrxml</field>
		<field name="menu">True</field>
		<field name="header">False</field>
		<field name="jasper_report">True</field>
		<field name="jasper_output">pdf</field>
		<field name="copies">3</field>
	</record>
	
	<record id="nstda_msd_invoice_values" model="ir.values">
		<field name="name">Invoice</field>
		<field name="model">account.invoice</field>
		<field name="key2" eval="'client_print_multi'" />
		<field name="value_unpickle" eval="'ir.actions.report.xml, ' +str(nstda_msd_invoice)" />
	</record>

</data>
</openerp>
